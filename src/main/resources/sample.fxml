<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vbox1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="428.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="Controller">

    <children>
        <AnchorPane fx:id="anchor1" prefHeight="428.0" prefWidth="371.0">
            <children>
                <Label fx:id="label1" alignment="CENTER" layoutX="50.0" layoutY="7.0" prefHeight="30.0"
                       prefWidth="273.0" text="Select Live Stream:" textAlignment="CENTER">
                    <font>
                        <Font size="22.0"/>
                    </font>
                </Label>
                <ComboBox fx:id="combo1" layoutX="25.0" layoutY="340.0" prefHeight="25.0" prefWidth="129.0"
                          promptText="Select Player">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Youtube"/>
                            <String fx:value="Streamlink"/>
                        </FXCollections>
                    </items>
                </ComboBox>

                <CheckBox fx:id="check1" layoutX="183.0" layoutY="340.0" mnemonicParsing="false" prefHeight="25.0"
                          prefWidth="105.0" text="Popout Chat"/>
                <Button fx:id="button1" layoutX="122.0" layoutY="382.0" mnemonicParsing="false"
                        onAction="#button1Clicked" prefHeight="38.0" prefWidth="129.0" text="Launch Stream">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Button>
                <ListView fx:id="listview1" layoutX="17.0" layoutY="47.0" prefHeight="276.0" prefWidth="344.0"/>
                <Button fx:id="button2" layoutX="298.0" layoutY="342.0" mnemonicParsing="false"
                        onAction="#button2Clicked" prefHeight="25.0" prefWidth="51.0" text="Neatclip">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>
